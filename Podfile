platform :ios, '9.0'

target 'ACKitSample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  def network_libs
      pod 'Alamofire'
  end

  def ui_libs
  end

  def model_libs
       pod 'ObjectMapper'
  end

  # Pods for ACKitSample
  network_libs
  ui_libs
  model_libs

  # Pods for ACKitSample

  target 'ACKitSampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ACKitSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
